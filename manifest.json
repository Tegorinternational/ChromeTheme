{
	"manifest_version": 3,
	"name": "ChromeTheme",
	"short_name": "ChromeTheme",
	"version": "3",
	"description": "ChromeTheme",
	"author": "Rurendra Tegor",
	"default_locale": "en",
	"theme_color": "#ffffff",
	"background_color": "#222222",
	"display": "standalone",
	"orientation": "portrait",
	"scope": "/",
	"start_url": "/",
	"prefer_related_applications": false,

	"icons": {
		"128": "icon.svg",
		"512": "icon.svg"
	},

	"chrome_url_overrides": {
		"newtab": "index.html"
	},

	"action": {
		"title": "ChromeTheme"
	},

	"offline_enabled": true
}
